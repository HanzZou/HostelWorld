<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.nju.hostelworld.model.ReservationEntity" table="reservation" schema="hostelworld">
        <id name="id">
            <column name="id" sql-type="char(10)" length="10"/>
        </id>
        <property name="memberId">
            <column name="member_id" sql-type="char(7)" length="7" not-null="true"/>
        </property>
        <property name="planId">
            <column name="plan_id" sql-type="char(10)" length="10" not-null="true"/>
        </property>
        <property name="isCanceled">
            <column name="is_canceled" sql-type="tinyint(1)" not-null="true"/>
        </property>
    </class>
</hibernate-mapping>