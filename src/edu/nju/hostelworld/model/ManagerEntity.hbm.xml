<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="edu.nju.hostelworld.model.ManagerEntity" table="manager" schema="hostelworld">
        <id name="id">
            <column name="id" sql-type="char(7)" length="7"/>
        </id>
        <property name="password">
            <column name="password" sql-type="varchar(255)"/>
        </property>
        <property name="name">
            <column name="name" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>